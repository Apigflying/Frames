/* 顶部信息 */
.header{
  width:750rpx;
  height:112rpx;
  background: #d33a32;
  position: fixed;
  top:0;
  left:0;  
}
.header-top{
  height:100%;
}
/* 内容部分 */
.box-wrap{
  width:750rpx;
  height:600rpx;
  border:1rpx solid black;
  box-sizing: border-box;
}
.box-wrap .box{
  width:100%;
  height:550rpx;
  margin-bottom:50rpx;
}
.box-wrap .box:last-of-type{
  margin-bottom:0;
}
.blue{
  background: blue;
}
.red{
  background: red;
}
.yellow{
  background: yellow;
}
.green{
  background: green;
}
/* 底部信息 */
.footer{
  position:fixed;
  bottom:0;
  left:0;
  height:100rpx;
  background: #fff;
  width:100%;
  height:102rpx;
  border-top:1rpx solid #5d544e;
}
.footer_img{
  margin:10rpx 0 0 10rpx;
  width:50rpx;
  height:50rpx;
  border-radius: 4rpx;
}